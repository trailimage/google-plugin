{
   "name": "@trailimage/google-provider",
   "version": "2.4.0",
   "description": "Trail Image Google provider",
   "private": false,
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/trailimage"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.6.0",
      "@toba/test": "^4.1.0",
      "typescript": "^3.5.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/trailimage/google-provider"
   },
   "bugs": {
      "url": "https://github.com/trailimage/google-provider/issues"
   },
   "dependencies": {
      "@toba/google-drive": "^1.6.0",
      "@toba/map": "^2.1.0",
      "@toba/node-tools": "^1.4.6",
      "@trailimage/models": "^2.2.0",
      "geojson": "^0.5.0"
   }
}
